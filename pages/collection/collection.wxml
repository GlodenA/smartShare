<!--pages/collection/collection.wxml-->
<view class="index" style="height:{{screenHeight}}px">
  <block wx:for="{{docsData}}"  wx:key="{{item}}">
    <view class="flex item">
      <view class="flex_auto item_left">
        <view><text class="title">{{item.DOC_NAME}}</text></view>
        <view><text class="sub_title">{{item.DOC_ID}}</text></view>
        <block wx:for="{{item.DOC_LABEL}}"  wx:key="{{index}} ">
        <view><text class="tag">{{item}}</text></view>
        </block>
         <view><text class="tag">{{item.DOC_AUTHOR_NAME}}</text></view>
        <view><text class="tag">{{item.INS_TIME}}</text></view>
      </view>
    </view>
  </block>
</view>
